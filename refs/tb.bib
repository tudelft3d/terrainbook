@article{Delaunay34,
	annote = {first paper by delaunay A well-known and important property of Delaunay triangulations is that ?local optimality? is equivalent to ?global optimality,? in the following sense. A hyperface, sandwiched between two d-simplices, is said to be locally Delaunay if no vertex of the two d-simplices lies inside the circumsphere of either d-simplex. (It is the apex of each d-simplex, not shared by the other d-simplex, that matters.) If a triangulation is Delaunay, every hyperface of the triangulation is locally Delaunay. Conversely, if every hyperface of a triangulation is locally Delaunay, then the triangulation is Delaunay (i.e. every simplex is Delaunay). Delaunay himself [13] was the first to make this observation.},
	author = {Delaunay, Boris N.},
